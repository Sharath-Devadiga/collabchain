generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String       @id @default(uuid())
  email        String       @unique
  createdAt    DateTime     @default(now())    
  username     String       @unique 
  githubId     Int          @unique
  accessToken  String 
}
